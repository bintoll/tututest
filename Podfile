  target 'tutuTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'SBJson', '~> 4.0.0'
  end

  target 'tutuUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'SBJson', '~> 4.0.0'
  end

end
